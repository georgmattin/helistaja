# Twilio Account Credentials
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here

# Twilio API Key for Voice SDK (required for access tokens)
TWILIO_API_KEY=your_api_key_here
TWILIO_API_SECRET=your_api_secret_here

# Twilio Phone Number (the number you purchased from Twilio)
TWILIO_PHONE_NUMBER=+1234567890

# TwiML Application SID (created in Twilio Console)
TWILIO_TWIML_APP_SID=your_twiml_app_sid_here

# Server Port (optional, defaults to 3000)
PORT=3000 